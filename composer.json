{
    "name": "kbunel/behat-test-generator",
	"type": "symfony-bundle",
    "description": "Project under development",
    "keywords": [ "test", "generator", "behat", "api" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bunel",
            "email": "kevinbunel@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "symfony/form": "4.1.*",
        "phpdocumentor/reflection-docblock": "^4.3",
        "doctrine/doctrine-bundle": "^1.9",
        "doctrine/orm": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "BehatTestGenerator\\": "./"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    }
}
