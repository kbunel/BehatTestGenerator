{
    "name": "kbunel/behat-test-generator",
    "type": "symfony-bundle",
    "description": "Generate automatic tests for all routes in you application, test that the response is good according to REST behavior.",
    "keywords": [ "test", "generator", "behat", "api" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bunel",
            "email": "kevinbunel@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "BehatTestGenerator\\": "./"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "behat/behat": "^3.5",
        "behat/symfony2-extension": "^2.1",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.3",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behatch/contexts": "^3.2"
    }
}
