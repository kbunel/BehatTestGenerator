{
    "name": "kbunel/BehatTestGenerator",
	"type": "symfony-bundle",
    "description": "API Test Generator for Behat test.",
    "keywords": [ "test", "generator", "behat", "api" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bunel",
            "email": "kevinbunel@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "symfony": "^3.4",
        "doctrine": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BehatTestGenerator\\": "./BehatTestGenerator",
            "BehatTestGenerator\\Command\\": "./BehatTestGenerator/Command"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    }
}
